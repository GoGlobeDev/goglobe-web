{
  "name": "factube-web-interface",
  "description": "Web interface of www.factube.com",
  "author": "factube.com",
  "license": "MIT",
  "version": "1.0.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/erikras/react-redux-universal-hot-example"
  },
  "homepage": "www.factube.com",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "webpack",
    "express",
    "hot reloading",
    "react-hot-reloader",
    "redux",
    "starter",
    "boilerplate",
    "babel"
  ],
  "main": "bin/server.js",
  "scripts": {
    "start": "concurrent --kill-others \"npm run start-prod\"",
    "start-prod": "better-npm-run start-prod",
    "build": "better-npm-run build",
    "postinstall": "npm run build",
    "lint": "eslint -c .eslintrc src",
    "start-dev": "better-npm-run start-dev",
    "watch-client": "better-npm-run watch-client",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\""
  },
  "betterScripts": {
    "start-prod": {
      "command": "forever start ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080,
        "APIPORT": 3030
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "build": {
      "command": "webpack --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "compression": "^1.6.0",
    "counterpart": "^0.18.6",
    "express": "^4.16.2",
    "immutability-helper": "^2.4.0",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.17.1",
    "piping": "^0.3.0",
    "pretty-error": "^1.2.0",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-bootstrap": "^0.31.3",
    "react-cookie": "^1.0.5",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.1.3",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-interpolate-component": "^0.12.0",
    "react-redux": "^5.0.6",
    "react-router": "^3.0.5",
    "react-router-redux": "^4.0.8",
    "react-translate-component": "^0.15.1",
    "redux": "^3.7.2",
    "redux-connect": "^5.1.0",
    "redux-thunk": "^2.1.0",
    "serialize-javascript": "^1.1.2",
    "serve-favicon": "^2.3.0",
    "video-react": "^0.9.4",
    "webpack-isomorphic-tools": "^3.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-display-name": "^6.25.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "better-npm-run": "0.0.15",
    "clean-webpack-plugin": "^0.1.16",
    "concurrently": "^0.1.1",
    "css-loader": "^0.28.4",
    "cssnano": "^3.10.0",
    "eslint": "1.10.3",
    "eslint-config-airbnb": "0.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^0.8.0",
    "eslint-plugin-react": "^3.5.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "json-loader": "^0.5.4",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "react-transform-catch-errors": "^1.0.0",
    "redbox-react": "^1.4.3",
    "redux-devtools-extension": "^2.13.2",
    "strip-loader": "^0.1.2",
